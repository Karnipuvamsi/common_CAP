
{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/fin/api/odata/(v4|v2)/app/(.*)$",
      "target": "/odata/$1/app/$2",
      "destination": "btpshesh_srv-api",
      "authenticationType": "none",
      "csrfProtection": true
    },
    {
      "source": "^/resources/ui5Lib/csc/ui5lib/delegates/(.*)$",
      "target": "/resources/ui5Lib/csc/ui5lib/delegates/$1",
      "destination": "btpshesh_srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },

    {
      "source": "^/(.*)$",
      "localDir": "ui5Lib"
    }
  ]
}
