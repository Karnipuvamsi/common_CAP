version: 0.0.1
modules:
- name: csc-sap-btp-ui5lib_dest-content
  type: com.sap.application.content
  requires:
  - name: csc-sap-btp-ui5lib_dest-srv
    parameters:
      content-target: true
  - name: csc-sap-btp-ui5lib_repo-host
    parameters:
      service-key:
        name: csc-sap-btp-ui5lib_repo-host-key
  parameters:
    content:
      instance:
        destinations:
        - Name: csc-sap-btp-ui5lib_repo_host
          ServiceInstanceName: csc-sap-btp-ui5lib_html5-srv
          ServiceKeyName: csc-sap-btp-ui5lib_repo-host-key
          sap.cloud.service: csc.sap.btp.ui5lib
        existing_destinations_policy: ignore
  build-parameters:
    no-source: true
- name: csc-sap-btp-ui5lib_app-content
  type: com.sap.application.content
  path: .
  requires:
  - name: csc-sap-btp-ui5lib_repo-host
    parameters:
      content-target: true
  build-parameters:
    build-result: resources
    requires:
    - name: sapbtpui5lib
      artifacts:
      - sapbtpui5lib.zip
      target-path: resources/
- name: sapbtpui5lib
  type: html5
  path: lib
  build-parameters:
    build-result: dist
    builder: custom
    commands:
    - npm install
    - npm run build:cf
    supported-platforms: []
resources:
- name: csc-sap-btp-ui5lib_dest-srv
  type: org.cloudfoundry.managed-service
  parameters:
    service: destination
    service-name: csc-sap-btp-ui5lib_dest-srv
    service-plan: lite
- name: csc-sap-btp-ui5lib_repo-host
  type: org.cloudfoundry.managed-service
  parameters:
    service: html5-apps-repo
    service-name: csc-sap-btp-ui5lib_html5-srv
    service-plan: app-host
parameters:
  deploy_mode: html5-repo
  enable-parallel-deployments: true





