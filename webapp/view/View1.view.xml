<mvc:View
    controllerName="protosample.controller.View1"
    height="100%"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified"
	xmlns:mdcvh="sap.ui.mdc.valuehelp"
	xmlns:mdcvc="sap.ui.mdc.valuehelp.content"
	xmlns:mdc="sap.ui.mdc"
	xmlns:table="sap.ui.mdc.table"
	xmlns:vm="sap.ui.fl.variants"
>
    <f:DynamicPage id="page">
        <f:content>
            <mdc:Table
               id="idMdcTableUsers" autoBindOnInit="true" width="100%" p13nMode="Sort,Column" selectionMode="SingleMaster" delegate="{name: 'sap/btp/ui5lib/delegates/DynamicTableDelegate', payload: {collectionPath: 'mainModel>/Customers'}}" threshold="50" filter="fbSelectBuildingDialog" enableAutoColumnWidth="true"
            >
            <table:Column
					propertyKey="customerName"
					header="customerName">
                    <Text text="{mainModel>customerName}"/>
            </table:Column>
            </mdc:Table>
        </f:content>
    </f:DynamicPage>
</mvc:View>
